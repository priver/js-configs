  "repository": "https://github.com/priver/linters/tree/master/packages/eslint-config",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "priver",
    "javascript",
    "styleguide"
  ],
  "author": "Mikhail Priver <m.priver@gmail.com> (https://github.com/priver)",
  "license": "MIT",
  "dependencies": {
    "eslint-restricted-globals": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-find-rules": "^3.3.1"
  },
  "peerDependencies": {
    "eslint": "^5.2.0"
  }
}
